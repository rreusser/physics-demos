{
  "name": "feynman-sum-over-path-quantum-theory",
  "version": "1.0.0",
  "description": "An interactive implementation of the paper, &#34;Teaching Feynman&#39;s sum-over-path quantum theory&#34;",
  "main": "index.js",
  "author": {
    "name": "Ricky Reusser",
    "email": "rsreusser@gmail.com"
  },
  "contributors": [
    {
      "name": "Ricky Reusser",
      "email": "rsreusser@gmail.com"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/rreusser/feynman-sum-over-path-quantum-theory.git"
  },
  "bugs": {
    "url": "https://github.com/rreusser/feynman-sum-over-path-quantum-theory/issues",
    "email": "rsreusser@gmail.com"
  },
  "scripts": {
    "lint": "npm run semistandard",
    "test": "npm run lint && mocha",
    "start": "node scripts/server.js | garnish",
    "tunnel": "lt --port 9966",
    "clean": "rm -rf dist",
    "build:staticjs": "browserify src/static.js | uglifyjs -cm > dist/bundle.js",
    "build:indexjs": "cp src/*.js dist/",
    "build:js": "npm run build:staticjs && npm run build:indexjs",
    "build:html": "node scripts/build.js --pattern=src/*.html --dest=dist/",
    "build:css": "cp src/*.css dist/",
    "build:fonts": "if [  -e src/fonts ]; then cp -r src/fonts dist/fonts; fi",
    "build:images": "if [ -e src/images ]; then cp -r src/images dist/images; fi",
    "build": "mkdir -p dist && npm run build:html && npm run build:js && npm run build:css && npm run build:fonts && npm run build:images",
    "build:nohtml": "mkdir -p dist && npm run build:js && npm run build:css && npm run build:fonts && npm run build:images",
    "deploy:gh": "npm run build && gh-pages -d dist",
    "deploy:s3": "npm run build:nohtml && node scripts/deploy-to-s3.js",
    "deploy:s3:snapshot": "npm run build:nohtml && node scripts/deploy-to-s3.js --snapshot"
  },
  "keywords": [],
  "devDependencies": {
    "budo": "^8.3.0",
    "chai": "^3.5.0",
    "copy-paste": "^1.3.0",
    "escape-html": "^1.0.3",
    "garnish": "^5.2.0",
    "gh-pages": "^0.11.0",
    "glob": "^7.0.5",
    "html": "^1.0.0",
    "hyperstream": "^1.2.2",
    "inject-lr-script-stream": "^1.1.1",
    "minimist": "^1.2.0",
    "mocha": "^3.0.2",
    "openurl": "^1.1.1",
    "s3": "^4.4.0",
    "simple-html-index": "^1.4.0",
    "stream-to-string": "^1.1.0",
    "string-to-stream": "^1.1.0",
    "through2": "^2.0.1",
    "xtend": "^4.0.1"
  },
  "dependencies": {
    "control-panel": "^1.2.0",
    "cwise": "^1.0.9",
    "d3": "^4.2.1",
    "event-emitter": "^0.3.4",
    "isndarray": "^1.0.0",
    "ndarray": "^1.0.18",
    "ndarray-complex": "^1.0.3",
    "ndarray-fill": "^1.0.1",
    "ndarray-householder-qr-complex": "^1.0.2",
    "ndarray-linspace": "^1.0.4",
    "ndarray-ops": "^1.2.2",
    "ndarray-pack": "^1.2.1",
    "ndarray-scratch": "^1.2.0",
    "ndarray-show": "^2.0.0",
    "ndarray-unpack": "^1.0.0",
    "node-pid-controller": "^1.0.0",
    "util-extend": "^1.0.3"
  },
  "semistandard": {
    "globals": [
      "describe",
      "it",
      "beforeEach"
    ]
  }
}
